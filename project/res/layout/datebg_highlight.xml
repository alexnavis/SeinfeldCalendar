<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
	<item android:top="1dp" android:bottom="1dp" android:left="1dp"
		android:right="1dp">
		<shape android:shape="rectangle">
			<solid android:color="#0A0A0A" />
			<corners android:bottomRightRadius="3dp"
				android:bottomLeftRadius="3dp" android:topLeftRadius="3dp"
				android:topRightRadius="3dp" />
		</shape>
	</item>
	<item android:top="1dp" android:bottom="25dp" android:left="1dp"
		android:right="1dp">
		<shape android:shape="rectangle">
			<gradient android:startColor="#22FFFFFF" android:endColor="#22636363"
				android:angle="270" />
			<corners android:radius="1dp" android:bottomRightRadius="0dp"
				android:bottomLeftRadius="0dp" android:topLeftRadius="3dp"
				android:topRightRadius="3dp" />
		</shape>
	</item>
	<item>
		<shape android:shape="line">
			<stroke  android:color="#FFFFFF" />
			<!--<size android:height="5sp" />
			--><!--<solid android:color="@color/dateSelectedBg"></solid>-->
		</shape>
	</item>
	<item android:top="8dp" android:bottom="8dp" android:left="8dp"
		android:right="8dp">
		<shape android:shape="rectangle">
			<!--<stroke android:width="2dp" android:color="#5500ffff" />-->
			<solid android:color="@color/dateSelectedBg"></solid>
			<corners android:bottomRightRadius="3dp"
				android:bottomLeftRadius="3dp" android:topLeftRadius="3dp"
				android:topRightRadius="3dp" />
		</shape>
	</item>
	<item android:top="8dp" android:bottom="28dp" android:left="8dp"
		android:right="8dp">
		<shape android:shape="rectangle">
			<gradient android:startColor="#66FFFFFF" android:endColor="#66636363"
				android:angle="270" />
			<corners android:radius="1dp" android:bottomRightRadius="0dp"
				android:bottomLeftRadius="0dp" android:topLeftRadius="3dp"
				android:topRightRadius="3dp" />
		</shape>
	</item>
</layer-list>

