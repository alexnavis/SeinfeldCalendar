<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
	<item android:top="1dp" android:bottom="1dp" android:left="1dp"
		android:right="1dp">
		<shape android:shape="rectangle">
			<solid android:color="#AA0A0A0A" />
			<corners android:bottomRightRadius="3dp"
				android:bottomLeftRadius="3dp" android:topLeftRadius="3dp"
				android:topRightRadius="3dp" />
		</shape>
	</item>
	<item android:top="1dp" android:bottom="25dp" android:left="1dp"
		android:right="1dp">
		<shape android:shape="rectangle">
			<gradient android:startColor="#22FFFFFF" android:endColor="#22636363"
				android:angle="270" />
			<corners android:bottomRightRadius="0dp"
				android:bottomLeftRadius="0dp" android:topLeftRadius="3dp"
				android:topRightRadius="3dp" />
		</shape>
	</item>
</layer-list>