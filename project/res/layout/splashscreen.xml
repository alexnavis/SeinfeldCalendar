<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="fill_parent" android:layout_height="fill_parent"
	android:orientation="vertical" android:background="#FFFFFFFF"
	android:id="@+id/splash">
    <TextView android:text="TextView" android:id="@+id/aboutContent" android:layout_height="wrap_content" android:layout_width="fill_parent" android:layout_marginTop="10sp" android:layout_marginBottom="10sp" android:textColor="#000000" android:gravity="center" android:layout_marginLeft="10sp" android:layout_marginRight="10sp"></TextView>
	<ImageView android:id="@+id/imageView1"
		android:layout_gravity="center_horizontal" android:src="@drawable/splash"
		android:layout_weight="1" android:layout_height="wrap_content"
		android:layout_width="wrap_content"></ImageView>
	<LinearLayout android:layout_height="wrap_content"
		android:id="@+id/linearLayout1" android:layout_width="fill_parent"
		android:background="@layout/splashauth_bg">
		<ImageView android:layout_gravity="center_vertical" android:id="@+id/imageView2" android:layout_width="wrap_content" android:layout_height="wrap_content" android:src="@drawable/pongal" android:layout_weight="1"></ImageView>
		<TextView
			android:text="Sudhakar Rayavaram\nVivek Ragunathan\nAlex Anto Navis Lawrence"
			android:id="@+id/textView1" android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:singleLine="false"
			android:layout_gravity="center_vertical" android:textColor="#000000" android:textSize="12sp" android:gravity="left|center_horizontal" android:layout_weight="3"></TextView>
	</LinearLayout>

</LinearLayout>
